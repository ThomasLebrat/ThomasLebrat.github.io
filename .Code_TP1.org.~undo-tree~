N = 1000 # Nombre de points
zmax = 100.0e3 # Altitude max (en m)
dz = zmax/(N-1) # Pas spatial (en m)

zatm = np.array([k*dz for k in range(N)]) # Altitudes
Tatm = np.array([T(zatm[k], 'K') for k in range(N)]) # Températures

TexpC = Texp + 273
