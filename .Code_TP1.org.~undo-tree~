# Calcul de la masse d’air jusqu’à l’altitude z
def masse_atm(z):
    masse = 0
    k = 0
    while zatm[k] < z: # On arrête le calcul à l’altitude z
        dm = 4*np.pi*(RT_m+zatm[k])**2*M*Patm[k]/(R*T(zatm[k], 'K')) * dz
        masse = masse + dm
        k = k + 1
    return masse
