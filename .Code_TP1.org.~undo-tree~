def T(z, unite):
    z_km = z / 1000 # Conversion en km pour comparaison dans la liste
    alpha = 1 # Valeur par défaut pour la conversion en K
    if unite == 'C':
        alpha = 0 # Pas de décalage pour la température en °C
    i = 0
    while z_km > zexp[i + 1]: # Recherche de l’indice i
        i = i + 1
    temperature = alpha*273 + Texp[i] + (z_km - zexp[i])/(zexp[i + 1] - zexp[i])*(Texp[i + 1] - Texp[i]) # Interpolation linéaire
    return temperature
 #+END_SRC" 0 1 (fontified nil org-category "Code_TP1" font-lock-fontified t font-lock-multiline t face org-block-begin-line org-reference 5114) 1 8 (fontified nil font-lock-fontified t org-category "Code_TP1" font-lock-multiline t face org-block-begin-line org-reference 5114) 8 12 (fontified nil font-lock-fontified t org-category "Code_TP1" face org-block-begin-line font-lock-multiline t org-reference 5114) 12 28 (fontified nil font-lock-fontified t org-category "Code_TP1" font-lock-multiline t face org-block-begin-line org-reference 5114) 28 60 (font-lock-fontified t fontified nil org-category "Code_TP1" font-lock-multiline t face org-block-begin-line org-reference 5114) 60 61 (font-lock-fontified t fontified nil rear-nonsticky t org-category "Code_TP1" font-lock-multiline t face org-block-begin-line org-reference 5114) 61 62 (fontified nil font-lock-fontified t org-category "Code_TP1" font-lock-multiline t face org-block-begin-line) 62 65 (fontified nil font-lock-fontified t src-block t org-category "Code_TP1" font-lock-multiline t face (font-lock-keyword-face (:background "thistle1" :extend t) org-block)) 65 66 (fontified nil font-lock-fontified t src-block t org-category "Code_TP1" font-lock-multiline t face ((:background "thistle1" :extend t) org-block)) 66 67 (fontified nil font-lock-fontified t src-block t org-category "Code_TP1" font-lock-multiline t face (font-lock-function-name-face (:background "thistle1" :extend t) org-block)) 67 83 (fontified nil font-lock-fontified t src-block t org-category "Code_TP1" font-lock-multiline t face ((:background "thistle1" :extend t) org-block)) 83 84 (fontified nil font-lock-fontified t src-block t org-category "Code_TP1" font-lock-multiline t face (font-lock-variable-name-face (:background "thistle1" :extend t) org-block)) 84 87 (fontified nil font-lock-fontified t src-block t org-category "Code_TP1" font-lock-multiline t face (font-lock-variable-name-face (:background "thistle1" :extend t) org-block)) 87 99 (fontified